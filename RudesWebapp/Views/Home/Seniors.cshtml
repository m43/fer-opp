@{
    ViewData["Title"] = "Seniori";
}

<div style="background-color: white;">
    <h4 style="font-size: 25px; font-family: 'Cinzel', serif; color: #A89435; font-weight: 800; margin-bottom: 20px; ">SENIORI 2019. /2020.</h4>
</div>

<div>
    <img src="~/images/timska-seniori.jpg" alt="Timska slika" style="        margin-bottom: 10px;
        min-width: 92%;
        display: block;
        margin: 0 auto;
        max-width: 100%;
" />
</div>

<div class="container" id="teams" style="padding: 0; margin-top: 20px;">
